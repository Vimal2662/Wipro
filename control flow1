import java.util.*;

class Main{
    public static void main(String...args)
    {
        Scanner Sc=new Scanner(System.in);
        int a=Sc.nextInt();

        if(a>0)
        {
            System.out.println("Positive Number");
        }
        else if(a==0)
        {
            System.out.println("Zero");
        }
        else{
        System.out.println("Negative Number");
        }
        
        }
}
